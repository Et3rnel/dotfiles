{
	"servers": {
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp"
			]
		},
		"fetch": {
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"mcp/fetch"
			],
			"type": "stdio"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		}
	},
	"inputs": []
}